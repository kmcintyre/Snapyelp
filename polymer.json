{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
  	"src/my-app.html",
    "src/my-index.html",    
    "src/my-tests.html",
    "src/my-test.html",
    "src/my-websocket.html",
    "src/shared-styles.html",
    "src/redux-mixin.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
  	"preset": "es5-bundled",
  	"addServiceWorker": false,
  	"addPushManifest": false
  }]
}
